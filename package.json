{
  "name": "ai-native-mall",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-old-space-size=8192 && next dev -p 3000",
    "build": "set NODE_OPTIONS=--max-old-space-size=8192 && next build",
    "start": "next start",
    "lint": "next lint",
    "dev:lint": "npm run lint",
    "dev:format": "npx eslint --fix . || true",
    "dev:check": "npm run dev:lint && npm run dev:format && npx tsc --noEmit",
    "generate-embeddings": "tsx scripts/generate-embeddings.ts",
    "test:email": "tsx scripts/test-email-verification.ts",
    "test:moderation": "tsx scripts/test-moderation.ts",
    "test:duplicate": "tsx scripts/test-duplicate.ts",
    "test:all": "tsx scripts/test-email-verification.ts && tsx scripts/test-moderation.ts",
    "test:e2e": "playwright test",
    "test:e2e:artifacts": "playwright test --reporter=list,html",
    "e2e:setup": "node scripts/e2e-setup.js",
    "setup:check": "tsx scripts/setup-check.ts",
    "demo:mock-flow": "tsx scripts/demo/mock_flow.ts",
    "test:e2e:sigv4": "tsx scripts/e2e/amazon_sigv4_e2e.ts",
    "test:e2e:ci": "tsx scripts/e2e/aws_sigv4_debug.ts",
    "test:unit": "vitest run --reporter dot",
    "test:unit:stable": "node --max-old-space-size=8192 node_modules/vitest/bin/vitest.js run --reporter dot --maxWorkers=50% --vmMemoryLimit=3072",
    "test:enhanced-security": "tsx scripts/test-enhanced-security.ts",
    "setup:supabase-bucket": "node scripts/supabase-create-bucket.js",
    "verify:supabase-bucket": "node scripts/supabase-verify-permissions.js",
    "initialize:phase3": "tsx scripts/initialize-phase3.ts",
    "test:phase3": "tsx scripts/test-phase3.ts",
    "test:scaling": "tsx scripts/test-scaling.ts",
    "test:cost-optimization": "tsx scripts/test-cost-optimization.ts",
    "sweep:watch": "node scripts/ci/watch-and-sweep.js",
    "watch:pr": "node scripts/watch-pr-ci.js --pr=16",
    "monitor:e2e": "tsx scripts/ci/monitor-e2e-runs.ts",
    "repro": "node scripts/repro-harness.js --cmd \"node scripts/sample-bundle-run.js --input scripts/samples/repro-input.json\" --input scripts/samples/repro-input.json",
    "repro:sample": "node scripts/repro-harness.js --cmd \"node scripts/sample-bundle-run.js --input scripts/samples/repro-input.json\" --input scripts/samples/repro-input.json --out repro",
    "repro:evolution": "node scripts/repro-harness.js --cmd \"npx tsx scripts/run-world-evolution.ts\" --out repro --workdir .",
    "ci:summarize-ndjson": "node scripts/ci-summarize-ndjson.js",
    "ci:targeted-repro": "node scripts/targeted-repro.js playwright-results.json 1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.2",
    "@azure/openai": "^1.0.0-beta.11",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.8",
    "@react-spring/three": "^10.0.3",
    "@react-three/drei": "^9.89.0",
    "@react-three/fiber": "^8.14.0",
    "@react-three/xr": "^6.6.28",
    "@sentry/nextjs": "^10.32.1",
    "@stripe/stripe-js": "^4.8.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@types/jest": "^30.0.0",
    "@types/three": "^0.182.0",
    "@xenova/transformers": "^2.17.2",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.25.0",
    "ioredis": "^5.9.1",
    "isomorphic-dompurify": "^2.16.0",
    "lucide-react": "^0.469.0",
    "next": "^15.5.9",
    "node-fetch": "^2.7.0",
    "ollama": "^0.3.0",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.2.0",
    "react-reconciler": "0.29.2",
    "recharts": "^2.15.0",
    "resend": "^6.7.0",
    "sharp": "^0.34.5",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.6",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.8",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/stripe": "^8.0.416",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "jsdom": "^22.1.0",
    "minimist": "^1.2.8",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.19.2",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@react-three/fiber": {
      "react-reconciler": "0.29.2"
    }
  }
}
